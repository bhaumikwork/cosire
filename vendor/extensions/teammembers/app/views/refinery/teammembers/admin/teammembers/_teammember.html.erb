<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(teammember) -%>">
  <span class='title'>
    <%= teammember.name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.teammembers_teammember_path(teammember),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_teammembers_admin_teammember_path(teammember),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.teammembers_admin_teammember_path(teammember),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => teammember.name),
        :method => :delete %>
  </span>
</li>
