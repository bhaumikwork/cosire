<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(digital) -%>">
  <span class='title'>
    <%= digital.name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.digitals_digital_path(digital),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_digitals_admin_digital_path(digital),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.digitals_admin_digital_path(digital),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => digital.name),
        :method => :delete %>
  </span>
</li>
